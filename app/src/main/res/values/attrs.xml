<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomViewSelectionBar">
        <attr name="barHeight" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="isBarRound" format="boolean" />
        <attr name="barCount" format="integer" />
        <attr name="barMode" format="enum">
            <enum name="fixed" value="0" />
            <enum name="stretch" value="1" />
        </attr>
    </declare-styleable>
</resources>